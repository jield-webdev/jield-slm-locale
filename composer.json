{
  "name": "jield-webdev/jield-slm-locale",
  "description": "Automatic detection of locales for Laminas",
  "type": "library",
  "license": "BSD-3-Clause",
  "keywords": [
    "laminas",
    "i18n",
    "locale"
  ],
  "homepage": "https://github.com/juriansluiman/SlmLocale",
  "authors": [
    {
      "name": "Jurian Sluiman",
      "email": "jurian@juriansluiman.nl",
      "homepage": "https://juriansluiman.nl"
    }
  ],
  "require": {
    "php": "~8.0.0 || ~8.1.0",
    "ext-intl": "*",
    "laminas/laminas-eventmanager": "^3.6",
    "laminas/laminas-http": "^2.17",
    "laminas/laminas-modulemanager": "^2.14",
    "laminas/laminas-router": "^3.10",
    "laminas/laminas-servicemanager": "^3.19",
    "laminas/laminas-stdlib": "^3.15",
    "laminas/laminas-view": "^2.25.0"
  },
  "require-dev": {
    "laminas/laminas-mvc": "^3.5",
    "php-coveralls/php-coveralls": "^2.5.3",
    "phpunit/phpunit": "^9.5",
    "phpspec/prophecy-phpunit": "v2.0.1"
  },
  "suggest": {
    "laminas/laminas-mvc": "For using the router in the UriPath strategy"
  },
  "autoload": {
    "psr-4": {
      "SlmLocale\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SlmLocaleTest\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "test": "phpunit --colors=always"
  }
}
